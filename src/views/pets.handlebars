<input type="button" onclick="location.href='/'" value="Registrar un producto">

<div class="prod-container">
    {{#each pets}}
    <div class="prod-card">
        <p>{{this.name}}</p>
        <p>{{this.specie}}</p>
        <p>{{this.age}}</p>
        <img src={{this.thumbnail}}>
    </div>
    {{/each}}
</div>